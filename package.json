{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"nodemon app.js\" \"npm run client\"",
    "client": "cd client && npm start",
    "production": "concurrently \"npm run server\" \"npm run serve\"",
    "server": "node app.js",
    "serve": "cd client && serve -s build -p 3000"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": "^0.18.0",
    "bluebird": ">=3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": ">=2.6.9",
    "dotenv": ">=6.2.0",
    "express": ">=4.16.4",
    "express-mongo-db": ">=2.0.4",
    "mongodb": ">=3.1.10",
    "morgan": "~1.0.0",
    "nodemon": "^1.18.9",
    "npm": "^6.8.0",
    "pdfkit": "^0.9.0",
    "request": ">=2.88.0"
  },
  "devDependencies": {
    "concurrently": ">=4.1.0"
  }
}
